
-- Chips and Other Resources

include base

%LIBRARY%
begin mod chips

export SN74LS08

-- Quad 2-Input AND
-- All inputs uninverted, bit, #USET by default
begin definition SN74LS08

INPUT vcc gnd a0 b0 a1 b1 a2 b2 a3 b3
OUTPUT c0 c1 c2 c3

-- type
TYPE i_vcc base::power::VCC
TYPE i_gnd base::power::GND

-- Requirements
REQIRE i_vcc i_gnd

begin set o_c0
compare i_a0 i_b0
$0
end o_c0

begin set o_c1
compare i_a1 i_b1
$0
end o_c1

begin set o_c2
compare i_a2 i_b2
$0
end o_c2

begin set o_c3
compare i_a3 i_b3
$0
end o_c3

end SN74LS08

end chipsw